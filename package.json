{
  "name": "lockin",
  "version": "0.1.0",
  "description": "LockIN project - consolidated monorepo",
  "engines": {
    "node": "24.13.0",
    "npm": "11.6.2"
  },
  "scripts": {
    "start": "node server/server.js",
    "server:dev": "nodemon server/server.js",
    "client:dev": "cd client && vite --config vite.config.ts",
    "client:build": "cd client && tsc -b ../tsconfig.app.json && vite build --config vite.config.ts --outDir dist",
    "client:preview": "cd client && vite preview --config vite.config.ts --port 5173",
    "dev": "concurrently \"npm:server:dev\" \"npm:client:dev\"",
    "build": "npm run client:build",
    "test:spaced": "node backEnd/test_spaced_repetition.js",
    "test:api": "jest --runInBand server/__tests__",
    "test": "npm run test:api",
    "client:test": "npx vitest run client/src",
    "e2e": "npx cypress open"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "knex": "^2.5.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "tailwindcss": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^8.2.0",
    "cypress": "^14.5.4",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jest": "^29.6.0",
    "jest-environment-node": "^29.6.0",
    "jsdom": "^22.1.0",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
